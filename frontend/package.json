{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky",
    "lint": "ng lint"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "^21.0.5",
    "@angular/cdk": "21",
    "@angular/common": "21",
    "@angular/compiler": "21",
    "@angular/core": "21",
    "@angular/forms": "21",
    "@angular/platform-browser": "21",
    "@angular/router": "21",
    "@iconify/json": "^2.2.337",
    "@iconify/tailwind4": "^1.0.6",
    "@microsoft/signalr": "^8.0.7",
    "@primeng/themes": "^21.0.2",
    "@types/leaflet": "^1.9.21",
    "leaflet": "^1.9.4",
    "primeicons": "^7.0.0",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "21",
    "@angular/cli": "21",
    "@angular/compiler-cli": "21",
    "@tailwindcss/postcss": "^4.1.6",
    "angular-eslint": "^21.1.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "primeng": "^21.0.2",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "bunx eslint"
    ],
    "*.{scss}": "bunx stylelint --fix"
  }
}
