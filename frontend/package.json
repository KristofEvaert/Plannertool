{
  "name": "planner",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky",
    "lint": "ng lint"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "^21.0.8",
    "@angular/cdk": "^21.0.6",
    "@angular/common": "^21.0.8",
    "@angular/compiler": "^21.0.8",
    "@angular/core": "^21.0.8",
    "@angular/forms": "^21.0.8",
    "@angular/platform-browser": "^21.0.8",
    "@angular/router": "^21.0.8",
    "@iconify/json": "^2.2.426",
    "@iconify/tailwind4": "^1.2.0",
    "@microsoft/signalr": "^8.0.17",
    "@primeng/themes": "^21.0.2",
    "@types/leaflet": "^1.9.21",
    "leaflet": "^1.9.4",
    "primeicons": "^7.0.0",
    "rxjs": "~7.8.2",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.8",
    "@tailwindcss/postcss": "^4.1.18",
    "angular-eslint": "^21.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-eslint": "https://pkg.pr.new/prettier-eslint@1197",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "primeng": "^21.0.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "bunx eslint"
    ],
    "*.{scss}": "bunx stylelint --fix"
  }
}